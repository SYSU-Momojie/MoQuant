<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.momojie.moquant.api.dao.MqForecastAdjustDao">
  <resultMap id="BaseResultMap" type="com.cn.momojie.moquant.api.dto.MqForecastAdjust">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="ts_code" jdbcType="VARCHAR" property="tsCode" />
    <result column="forecast_type" jdbcType="INTEGER" property="forecastType" />
    <result column="end_date" jdbcType="VARCHAR" property="endDate" />
    <result column="dprofit" jdbcType="DECIMAL" property="dprofit" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="one_time" jdbcType="BIT" property="oneTime" />
  </resultMap>

    <select id="selectOne" resultMap="BaseResultMap">
        SELECT * FROM mq_forecast_adjust
        WHERE 1=1
        <if test="ts_code != null">
            AND ts_code = #{ts_code}
        </if>
        <if test="end_date != null">
            AND end_date = #{end_date}
        </if>
        <if test="forecast_type != null">
            AND forecast_type = #{forecast_type}
        </if>
    </select>
</mapper>